{
  "name": "workbunny/php-orc",
  "description": "PHP library for reading and writing Apache ORC file format. It uses the swoole/phpy extension to call the Python module PyORC for implementation.",
  "type": "library",
  "keywords": [
    "php-orc",
    "apache orc",
    "phpy"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "chaz6chez",
      "email": "chaz6chez1993@outlook.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/workbunny/php-orc/issues",
    "source": "https://github.com/workbunny/php-orc"
  },
  "require": {
    "php": ">=8.1",
    "symfony/console": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "Workbunny\\PhpOrc\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "suggest": {
    "swoole/phpy": "For use PyOrc to read and write Apache ORC file format."
  },
  "require-dev": {
    "symfony/var-dumper": "^7.2",
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "swoole/phpy": "dev-main"
  },
  "bin": [
    "bin/php-orc"
  ]
}
